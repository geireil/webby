lock;\r\n\tpadding: 15px;\r\n\theight: 160px;\r\n\tbox-shadow:4px 4px 4px #eaeaea;\r\n}\r\n\r\nh3{\r\n\tpadding-bottom: 5px;\r\n\tfont-weight: bold;\r\n}\r\n\r\n.cardText{\r\n\tpadding-bottom: 25px;\r\n\theight: 70px;\r\n}\r\n\r\n.deleteBtn{\r\n\twidth: 30%;\r\n\tborder-radius: 4px;\r\n\tpadding: 2px 6px;\r\n\tborder: 0;\r\n\ttext-align: center;\r\n\tcolor: #ffffff;\r\n\tbackground: #fd4646;\r\n\tfloat: right;\r\n}\r\n\r\n.deleteBtn:hover{\r\n\tbackground: #fa8787;\r\n}\r\n\r\n.completeBtn{\r\n\tmargin-left: 10px;\r\n\twidth: 30%;\r\n\tborder-radius: 4px;\r\n\tpadding: 2px 6px;\r\n\tborder: 0;\r\n\ttext-align: center;\r\n\tcolor: #ffffff;\r\n\tbackground: rgb(54, 212, 191);\r\n\tfloat: right;\r\n}\r\n\r\n.completeBtn:hover{\r\n\tbackground: rgb(149, 227, 217);\r\n}\r\n\r\n#dateFilter{\r\n\ttext-align: right;\r\n\tpadding: 10px 2px;\r\n}\r\n\r\n#filterWrapper{\r\n\ttext-align: right;\r\n\tpadding: 10px 2px;\r\n\twidth: 100%;\r\n}\r\n\r\nul{\r\n\twidth: 100%;\r\n\tlist-style-type: none;\r\n}\r\n\r\nul li{\r\n\twidth: 100%;\r\n\tborder-bottom: 1px solid #cdcdcd;\r\n\tpadding: 10px 2px;\r\n}\r\n\r\nul li span{\r\n\tdisplay:inline-block;\r\n\twidth: 19%;\r\n}\r\n\r\n.description{\r\n\twidth: 49%;\r\n}\r\n\r\n.completedDate{\r\n\twidth: 11%;\r\n\ttext-align: right;\r\n}\r\n\r\n#topLi{\r\n\tbackground: rgb(168, 168, 168);\r\n}\r\n\r\n#topLi span{\r\n\tfont-weight: bold;\r\n}\r\n\r\n\r\n\r\n /* The Modal (background) */\r\n .modal {\r\n\tdisplay: none; /* Hidden by default */\r\n\tposition: fixed; /* Stay in place */\r\n\tz-index: 1; /* Sit on top */\r\n\tleft: 0;\r\n\ttop: 0;\r\n\twidth: 100%; /* Full width */\r\n\theight: 100%; /* Full height */\r\n\toverflow: auto; /* Enable scroll if needed */\r\n\tbackground-color: rgb(169, 169, 169); /* Fallback color */\r\n\tbackground-color: rgba(229, 229, 229, 0.771); /* Black w/ opacity */\r\n}\r\n  \r\n/* Modal Content/Box */\r\n.modal-content {\r\n\tbackground-color: #fefefe;\r\n\tmargin: 15% auto; /* 15% from the top and centered */\r\n\tpadding: 0px;\r\n\twidth: 20%; /* Could be more or less, depending on screen size */\r\n\tborder-radius: 6px;\r\n\tbox-shadow: 0 0 5px rgb(222, 220, 220);\r\n}\r\n  \r\n/* The Close Button */\r\n.close {\r\n\tcolor: #aaa;\r\n\tfont-size: 28px;\r\n\tfont-weight: bold;\r\n}\r\n  \r\n.close:hover, .close:focus {\r\n\tcolor: black;\r\n\ttext-decoration: none;\r\n\tcursor: pointer;\r\n}\r\n\r\n#form-headertitle{\r\n\tdisplay: inline-block;\r\n\twidth: 49%;\r\n\tmargin: 0px;\r\n\ttext-align: left;\r\n\tfont-weight: bold;\r\n}\r\n\r\n#form-close{\r\n\tdisplay: inline-block;\r\n\twidth: 49%; \r\n\tmargin: 0px;\r\n\ttext-align: right;\r\n\tpadding-bottom: 5px;\r\n}\r\n\r\n#form-header{\r\n\tborder-bottom: 2px solid #dedede9c;\r\n\tpadding: 0 8px;\r\n\tmargin: 0px 0px 26px 0px;\r\n}\r\n\r\n#form-title{\r\n\tmargin: 4px 0px 20px 0px;\r\n\twidth: 100%;\r\n\tpadding: 4px 4px;\r\n}\r\n\r\n#form-description{\r\n\tmargin: 4px 0px 20px 0px;\r\n\twidth: 100%;\r\n\tpadding: 4px 4px;\r\n}\r\n\r\n#charactersLeft{\r\n\tcolor: #919191;\r\n}\r\n\r\n#form-author{\r\n\tmargin: 4px 0px 20px 0px;\r\n\twidth: 100%;\r\n\tpadding: 4px 4px;\r\n}\r\n\r\n#form-createBtn{\r\n\tmargin: 0px 0px 40px 0px;\r\n\twidth: 100%;\r\n\tborder-radius: 1px;\r\n\tpadding: 6px 6px;\r\n\ttext-align: center;\r\n\tcolor: #ffffff;\r\n\tbackground: rgb(54, 212, 191);\r\n\tborder: 0;\r\n}\r\n\r\n#form-container{\r\n\tmargin: auto;\r\n\twidth: 80%;\r\n}\r\n\r\nform label{\r\n\tfont-size: 12px;\r\n\tfont-weight: bold;\r\n}\r\n\r\n\r\n"},"lineCount":null}},"error":null,"hash":"61e614adf06c63226104aa86d58670e2","cacheData":{"env":{}}}